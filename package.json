  "dependencies": {
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.6.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jquery": "^3.5.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.12",
    "morgan": "^1.10.0",
    "node-sass": "^4.14.0",
    "sass-loader": "^8.0.2",
    "slugify": "^1.4.0",
    "validator": "^13.0.0",
    "vue": "^2.6.11",
    "vue-toasted": "^1.1.28",
    "vuelidate": "^0.7.5",
    "vuex": "^3.3.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.3.0",
    "@vue/cli-plugin-eslint": "^4.3.0",
