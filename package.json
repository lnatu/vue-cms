    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue-plugin-load-script": "^1.2.0",
    "vue-router": "^3.1.6",
    "vue-template-compiler": "^2.6.11"
